{
	"name": "grifart/stateful",
	"type": "library",
	"description": "Serializes any object into primitive types and arrays.",
	"require": {
		"php": ">= 7.1.0",
		"php-ds/php-ds": "^1.1",
		"grifart/comparable-polyfill": "^1.0",
		"grifart/equalable-utils": "^1.0"
	},
	"require-dev": {
		"nette/tester": "^2.0.0",
		"mockery/mockery": "^1.0.0"
	},
	"autoload": {
		"psr-4": {
			"Grifart\\Stateful\\": "src/"
		},
		"files": ["src/exceptions.php"]
	},
	"autoload-dev": {
		"psr-4": {
			"Grifart\\Stateful\\": "tests/Stateful"
		}
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "git@gitlab.grifart.local:grifart-internal/comparable-polyfill.git"
		},
		{
			"type": "vcs",
			"url": "git@gitlab.grifart.local:grifart-internal/equalable-utils.git"
		}
	]
}
